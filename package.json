{
  "name": "dinosauregg",
  "version": "1.0.0",
  "description": "Common scaffolding based on egg.js construction",
  "dependencies": {
    "egg": "^2.26.0"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.14.1",
    "egg-ci": "^1.13.1",
    "egg-mock": "^3.25.1",
    "eslint": "^6.8.0",
    "eslint-config-egg": "^8.0.1",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && egg-bin pkgfiles --check && npm run cov",
    "autod": "autod",
    "pkgfiles": "egg-bin pkgfiles"
  },
  "ci": {
    "version": "8, 9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idrex/dinosauregg.git"
  },
  "keywords": [
    "egg",
    "egg-framework"
  ],
  "author": "Drex",
  "files": [
    "index.js",
    "lib",
    "app",
    "config"
  ],
  "eslintIgnore": [
    "coverage",
    "dist"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idrex/dinosauregg/issues"
  },
  "homepage": "https://github.com/idrex/dinosauregg#readme"
}
